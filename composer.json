{
    "name": "touiliy/docraptor-bundle",
    "type": "symfony-bundle",
    "description": "Bundle around the Docraptor API client",
    "keywords": ["docraptor", "api", "symfony3", "bundle"],
    "homepage": "https://github.com/touiliy/docraptor-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Martijn Dwars",
            "email": "ikben@martijndwars.nl"
        }
    ],

    "repositories": [
        {
            "type":"vcs",
            "url":"https://github.com/touiliy/docraptor-bundle"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "3.1.*",
        "bytes/docraptor":"dev-master"
    },
    "autoload": {
        "psr-0": { "Bytes\\Bundle\\DocraptorBundle": "" }
    },
    "target-dir": "Bytes/Bundle/DocraptorBundle"
}
